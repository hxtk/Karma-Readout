{
  "manifest_version": 2,

  "name": "Karma Readout",
  "description": "Restore detailed karma readout",
  "version": "1.1",

  "permissions": [
    "http://*.reddit.com/r/*",
    "https://*.reddit.com/r/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.reddit.com/r/*",
                  "https://*.reddit.com/r/*" 
        ],
      "js": [
            "jquery.min.js",
            "script.js"
      ]
    }
  ]
}
